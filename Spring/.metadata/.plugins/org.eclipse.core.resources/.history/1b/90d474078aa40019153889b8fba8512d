package com.bridgeit.bean;

public class Customer {
	private Person person;
	private int type;
	private String action;
	public void setPerson(Person person) {
		this.person = person;
	}
	public void setType(int type) {
		this.type = type;
	}
	public void setAction(String action) {
		this.action = action;
	}
}
